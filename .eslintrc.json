{   
    "parser": "@babel/eslint-parser",
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "google",
        "plugin:import/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["/public/app.js"],
    "rules": {
        "quotes": "off",
        "no-trailing-spaces":"off",
        "max-len": ["error", 150],
        "require-jsdoc": "off",
        "comma-dangle": "off",
        "comma-spacing": "off",
        "indent":"off",
        "eol-last":"off",
        "semi":"warn",
        "no-console": 2,
        "padded-blocks":"off",
        "no-unused-vars":"off",
        "object-curly-spacing": "off"
    }
}
